{
    "name": "meritoo/limesurvey-api-client",
    "description": "Client of LimeSurvey API",
    "type": "library",
    "license": "MIT",
    "version": "0.0.9",
    "authors": [
        {
            "name": "Meritoo",
            "email": "github@meritoo.pl"
        }
    ],
    "require": {
        "php": ">=5.4",
        "fguillot/json-rpc": "^1.2",
        "gedmo/doctrine-extensions": "^2.4",
        "symfony/http-foundation": "^2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "^3.1",
        "phpmd/phpmd": "^2.6",
        "sebastian/phpcpd": "^2.0",
        "pdepend/pdepend": "^2.5",
        "phploc/phploc": "^2.1",
        "friendsofphp/php-cs-fixer": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Meritoo\\Common\\": "src/Common/",
            "Meritoo\\LimeSurvey\\ApiClient\\": "src/LimeSurvey/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Meritoo\\LimeSurvey\\Test\\ApiClient\\": "tests/"
        }
    }
}
